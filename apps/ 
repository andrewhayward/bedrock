marketplace/views.py:1:import l10n_utils
marketplace/views.py:2:from django.conf import settings
marketplace/views.py:3:from django.core.validators import email_re
marketplace/views.py:5:import basket
marketplace/views.py:7:def marketplace(request):
marketplace/views.py:8:    submitted = False
marketplace/views.py:9:    error = False
marketplace/views.py:10:    email = ''
marketplace/views.py:12:    if request.method == 'POST':
marketplace/views.py:13:        email = request.POST['email']
marketplace/views.py:14:        format_ = 'T' if request.POST.get('format') == 'text' else 'H'
marketplace/views.py:15:        newsletter = 'app-dev'
marketplace/views.py:17:        if not email_re.match(email):
marketplace/views.py:18:            error = 'email'
marketplace/views.py:20:        if not request.POST.get('privacy', None):
marketplace/views.py:21:            error = 'privacy'
marketplace/views.py:23:        if not error:
marketplace/views.py:24:            basket.subscribe(email, newsletter, format=format_)
marketplace/views.py:25:            submitted = True
marketplace/views.py:26:            
marketplace/views.py:27:    return l10n_utils.render(request,
marketplace/views.py:28:                             "marketplace/marketplace.html",
marketplace/views.py:29:                             {'submitted': submitted,
marketplace/views.py:30:                              'error': error,
marketplace/views.py:31:                              'email': email})
